
@{
    ViewData["Title"] = "ManageSequence";
}
<div class="content-wrapper">
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0 text-dark">Manage Sequence</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item">
                            <a asp-action="Index" asp-controller="Dashboard">Home</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a asp-action="List" asp-controller="Menu">Menu</a>
                        </li>
                        <li class="breadcrumb-item active">Menu Item Sequence</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>

    <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Menu Item List</h3>
                        </div>
                        <div class="card-body">
                            <table id="menuItemDetails" class="table table-bordered table-striped w-100">
                                <thead>
                                    <tr>
                                        <th class="no-sort text-center">Sequence</th>
                                        <th class="no-sort">Menu Item Name</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts {
    <script src="~/custom-script/ItemSequence.js"></script>
}

